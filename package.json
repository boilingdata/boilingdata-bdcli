{
  "name": "bdcli",
  "version": "0.0.3",
  "description": "BoilingData command-line client",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">= 16"
  },
  "author": "Dan Forsberg <dan@boilingdata.com>",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version",
    "prebuild": "rm -rf dist/ && echo \"export const version = \\\"$(git describe --tags)\\\";\" > src/VERSION.ts",
    "build": "tsc",
    "bdcli": "node -r ts-node/register src/index.ts",
    "prettier": "prettier --check 'src/**/*.{js,ts}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,ts}'",
    "lint": "yarn prettier && eslint \"./src/**/*.ts\"",
    "lint:fix": "yarn lint --quiet --fix",
    "lint:watch": "npx onchange 'src/**/*.ts' -- yarn lint"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^6.2.0",
    "chalk": "^5.0.0",
    "commander": "^10.0.1",
    "ora": "^6.3.1",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^10.0.3",
    "@tsconfig/node-lts": "^18.12.2",
    "@types/node": "^18.8.0",
    "@types/ora": "^3.2.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
