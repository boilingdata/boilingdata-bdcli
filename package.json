{
  "name": "bdcli",
  "version": "0.0.2",
  "description": "BoilingData command-line client",
  "main": "index.js",
  "author": "Dan Forsberg <dan@boilingdata.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version",
    "prebuild": "echo \"export const version = \\\"$(git describe --tags)\\\";\" > src/VERSION.ts",
    "build": "ncc build src/index.ts -o dist/",
    "run": "ts-node src/index.ts",
    "prettier": "prettier --check 'src/**/*.{js,ts}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,ts}'",
    "lint": "yarn prettier && eslint \"./src/**/*.ts\"",
    "lint:fix": "yarn lint --quiet --fix",
    "lint:watch": "npx onchange 'src/**/*.ts' -- yarn lint"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^6.2.0",
    "commander": "^10.0.1"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^10.0.3",
    "@types/node": "^18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.41.0",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
