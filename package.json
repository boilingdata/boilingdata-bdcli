{
  "name": "bdcli",
  "version": "0.0.5",
  "description": "BoilingData command-line client",
  "exports": "./dist/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "engines": {
    "node": ">= 16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boilingdata/boilingdata-bdcli"
  },
  "keywords": [
    "BoilingData",
    "AWS",
    "Big Data & Analytics"
  ],
  "author": "Dan Forsberg <dan@boilingdata.com>",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version",
    "prebuild": "yarn install --frozen-lockfile && rm -rf dist/ && echo \"/* istanbul ignore file */\nexport const version = \\\"$(git describe --tags)\\\";\" > src/VERSION.ts && ts-interface-builder src/integration/boilingdata/dataset.interface.ts ",
    "build": "rm -rf dist/ && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && chmod 755 dist/esm/index.js && ln -fs dist/esm/index.js bdcli",
    "prettier": "prettier --check 'src/**/*.{js,ts}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,ts}'",
    "lint": "yarn prettier && eslint \"./src/**/*.ts\"",
    "lint:fix": "yarn lint --quiet --fix",
    "lint:watch": "npx onchange 'src/**/*.ts' -- yarn lint",
    "test": "jest",
    "test:watch": "yarn test -- --watchAll",
    "test:badges": "npx jest-coverage-badges output ./badges"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "dependencies": {
    "@aws-sdk/client-iam": "3.350.0",
    "@aws-sdk/client-sts": "3.350.0",
    "amazon-cognito-identity-js": "^6.2.0",
    "chalk": "^5.0.0",
    "commander": "^10.0.1",
    "js-yaml": "^4.1.0",
    "ora": "^6.3.1",
    "ts-interface-checker": "^1.0.2",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^10.0.3",
    "@swc/core": "^1.3.62",
    "@swc/jest": "^0.2.26",
    "@tsconfig/node-lts": "^18.12.2",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "aws-sdk-client-mock": "^2.1.1",
    "eslint": "^8.41.0",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "ts-interface-builder": "^0.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
